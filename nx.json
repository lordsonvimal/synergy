{
  "installation": {
    "version": "20.4.6"
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/go.work"
    ]
  },
  "targetDefaults": {
    "serve": {
      "dependsOn": [
        "^serve"
      ],
      "cache": true
    },
    "nx": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "e2e-ci--**/*": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "plugins": [
    "@nx-go/nx-go",
    "@nx/js",
    "@nx/web",
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "targetName": "e2e"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ]
}
